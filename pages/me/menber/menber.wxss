/***********************vip添加包*************************/
.vip-add .list{
    width: 32%;
    color: #fff;
}

.vip-add .list:not(:last-child){
    margin-right: 2%;
}
.vip-add .scroll {height: 80px;}
.powerList.scroll{height: 90px;}


::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}


/*****用户会员特权*******/
.power-list{
    display: inline-block;
    width: 28%;
    height: 90px;
    font-size: 10px;
    text-align: center;
    vertical-align:top;
}
.power-list>view{
  width: 100%;
  text-align: center;
  white-space:normal;
  color: #495060;
}
.power-list>view.powerimg{
    width: 40px;
    height: 40px;
    margin: 0 auto 8px;
    background: #E5C187;
    /* background: #5cadff; */
    border-radius: 50%;
}
.power-list>view>image{
    width: 30px;
    height: 30px;
    margin-top: 5px;
}

.provision{
    /*margin: 0 0 10px;*/
    color: #80848f;
    padding: 0 10px 15px;
    font-size: 12px;

}
.btn{padding-top: 6px;}
.provision>view{
    display: inline-block;
    color: #5cadff;

}
.actionsModal{
    text-align: center;
    width: 70%;
    margin: 0 auto;
}

